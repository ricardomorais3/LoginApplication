<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="userDao" class="org.academiadecodigo.loginSceneBuilder.model.dao.hibernate.HibernateUserDao">
        <property name="hibernateSessionManager" ref="hibernateSessionManager"/>
    </bean>

    <bean id="roleDao" class="org.academiadecodigo.loginSceneBuilder.model.dao.hibernate.HibernateRoleDao">
        <property name="hibernateSessionManager" ref="hibernateSessionManager"/>
    </bean>

</beans>